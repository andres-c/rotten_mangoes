<h1>Admin page</h1>
<%= link_to "Home!", movies_path %>
<%= link_to "Create a User!", new_admin_user_path %>

<% @users.each do |user| %>
<table>
  <tr>
    <td>
      <%= user.firstname %>
    </td> 
  <tr>
  <tr>
    <td><%= link_to "edit", edit_admin_user_path(user)%></td>
    <td><%= link_to "delete", admin_user_path(user), method: :delete, confirm: "You sure?" %></td>
  </tr>   
  <% end %>
</tr>

</table>

<%= paginate  @users %>